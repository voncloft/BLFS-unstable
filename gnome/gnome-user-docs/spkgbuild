# description   : The GNOME User Docs package contains documentation for GNOME
# depends       : itstool libxml2

name=gnome-user-docs
version=48.2    
release=1
source="https://download.gnome.org/sources/gnome-user-docs/48/gnome-user-docs-$version.tar.xz"

options="!checksum"

build() {
    cd $name-$version
    
    ./configure --prefix=/usr 
    make
    make DESTDIR=$PKG install
}
