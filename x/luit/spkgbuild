# description   : The luit package is a filter that can be run between an arbitrary application and a UTF-8 terminal emulator
# depends       : xorg applications

name=luit
version=20240910    
release=1
source=""

options="!checksum"

build() {
    cd $name-$version
    
    ./configure $XORG_CONFIG 
    make
    make DESTDIR=$PKG install
}
