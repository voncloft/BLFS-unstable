# description   : The GNOME Icon Theme Extras package contains extra icons for the GNOME Desktop
# depends       : gnomeicontheme

name=gnome-icon-theme-extras
version=3.12.0    
release=1
source="https://download.gnome.org/sources/gnome-icon-theme-extras/3.12/gnome-icon-theme-extras-$version.tar.xz"

options="!checksum"

build() {
    cd $name-$version
    
    ./configure --prefix=/usr 
    make
    make DESTDIR=$PKG install
}
