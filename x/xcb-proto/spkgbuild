# description   : The xcb-proto package provides the XML-XCB protocol descriptions that libxcb uses to generate the majority of its code and API
# depends       : settingupthexorgbuildenvironment

name=xcb-proto
version=1.17.0    
release=1
source="https://xorg.freedesktop.org/archive/individual/proto/xcb-proto-$version.tar.xz"

options="!checksum"

build() {
    cd $name-$version
    
    PYTHON=python3 ./configure $XORG_CONFIG
    rm -f $XORG_PREFIX/lib/pkgconfig/xcb-proto.pc
    make DESTDIR=$PKG install
}
