# description   : The GOffice package contains a library of GLib/GTK document centric objects and utilities
# depends       : gtk libgsf librsvg libxslt which

name=goffice
version=0.10.59    
release=1
source="https://download.gnome.org/sources/goffice/0.10/goffice-$version.tar.xz"

options="!checksum"

build() {
    cd $name-$version
    
    ./configure --prefix=/usr 
    make
    make DESTDIR=$PKG install
}
