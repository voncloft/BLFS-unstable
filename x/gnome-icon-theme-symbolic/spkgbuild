# description   : The GNOME Icon Theme Symbolic package contains symbolic icons for the default GNOME icon theme
# depends       : gnomeicontheme

name=gnome-icon-theme-symbolic
version=3.12.0    
release=1
source="https://download.gnome.org/sources/gnome-icon-theme-symbolic/3.12/gnome-icon-theme-symbolic-$version.tar.xz"

options="!checksum"

build() {
    cd $name-$version
    
    ./configure --prefix=/usr 
    make
    make DESTDIR=$PKG install
}
