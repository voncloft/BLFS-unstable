# description   : The ISO Codes package contains a list of country, language and currency names and it is used as a central database for accessing this data
# depends       : 

name=iso codes
version=4.18.0    
release=1
source="https://salsa.debian.org/iso-codes-team/iso-codes/-/archive/v$version/iso-codes-v$version.tar.gz"

options="!checksum"

build() {
    cd $name-$version
    
    ./configure --prefix=/usr 
    make
    make DESTDIR=$PKG install LN_S='ln -sfn'
}
