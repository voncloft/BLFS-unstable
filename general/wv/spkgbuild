# description   : The wv package contains tools for reading information from an MS Word document
# depends       : libgsf libpng

name=wv
version=1.2.9    
release=1
source="https://anduin.linuxfromscratch.org/BLFS/wv/wv-$version.tar.gz"

options="!checksum"

build() {
    cd $name-$version
    
    ./configure --prefix=/usr --disable-static 
    make
    make DESTDIR=$PKG install
}
