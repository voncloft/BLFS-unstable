# description   : No description available
# depends       : 

name=about console fonts
version=
release=1
source=""

options="!checksum"

build() {
    cd $name-$version
    
    setfont /path/to/yourfont.ext
    setfont gr737a-9x16
    showconsolefont
    make DESTDIR=$PKG psf
    install -v -m644 ter-v32n.psf.gz /usr/share/consolefonts
}
