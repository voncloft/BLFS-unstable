# description   : The Menu Cache package contains a library for creating and utilizing caches to speed up the manipulation for freedesktop
# depends       : gtkdoc libfmextra

name=menu-cache
version=1.1.1    
release=1
source="https://github.com/lxde/menu-cache/archive/$version/menu-cache-$version.tar.gz"

options="!checksum"

build() {
    cd $name-$version
    
    sh autogen.sh                              
    ./configure --prefix=/usr --disable-static 
    make
    make DESTDIR=$PKG install
}
